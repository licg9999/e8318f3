{
  "endOfLine": "auto",
  "printWidth": 100,
  "singleQuote": true,
  "tabWidth": 2,
  "useTabs": false,
  "importOrder": ["^node:", "<THIRD_PARTY_MODULES>", "^\\."],
  "importOrderCaseInsensitive": true,
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "overrides": [
    {
      "files": ["src/**/*.json"],
      "options": {
        "parser": "json-stringify"
      }
    }
  ]
}
